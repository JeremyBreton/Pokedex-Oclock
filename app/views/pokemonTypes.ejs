<%- include('partials/header') %>

<h2>Pokemons de type <%= pokemons[0].type_name %></h2>

    <div class="container">
        <% for (const pokemon of pokemons) { %>
            <div class="pokemon_box">
                <a class="linkPokemon" href="/pokemon/<%=pokemon.numero%>">
                    <img src="/img/<%= pokemon.numero %>.png">
                    <p class="pokemon_title"> #<%= pokemon.numero %> <%= pokemon.nom %> </p>
                </a>
            </div>  
        <% } %>
    </div>

<%- include('partials/footer') %>